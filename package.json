{
  "name": "turboaery",
  "version": "0.0.1",
  "description": "Assistant tool for League players",
  "main": "public/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "pack": "rescripts build && electron-builder -wm --dir",
    "dist": "rescripts build && electron-builder -wm",
    "electron-dev": "node scripts/electron-dev.js"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "zip"
    },
    "mac": {
      "target": "zip"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dixaba/TurboAery.git"
  },
  "author": "Dixaba <dixaba@yandex.ru> (https://github.com/Dixaba)",
  "contributors": [
    "Arseny Kruglikov <kruglikov.arseny@gmail.com> (https://github.com/arsenykruglikov)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dixaba/TurboAery/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "electron-is-dev": "^1.1.0",
    "lcu-connector": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.10",
    "@rescripts/rescript-env": "0.0.10",
    "concurrently": "^4.1.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.40.2",
    "wait-on": "^3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
